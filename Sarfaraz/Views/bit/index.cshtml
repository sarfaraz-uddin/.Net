@model List<Sarfaraz.Models.Std>
@{
    ViewBag.Title = "index";
    int sn = 0;
}
<style>
    .table{
       background-color:black;
       color:white;
    }
</style>
<h2>index</h2>
<table class="table" border="1">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Address</th>
        <th>Contact</th>

    </tr>
    @foreach(var data in Model)
            {
                <tr>
                    <td>@(++sn)</td>
                    <td>@(data.name)</td>
                    <td>@(data.address)</td>
                    <td>@(data.Phone_No)</td>
                </tr>
            }
</table>
