@model Sarfaraz.Models.doctor
@{
    ViewBag.Title = "edit1";
}

<h2>edit1</h2>
@using (Html.BeginForm("Updatedata", "bit2"))
{
    <input type="hidden" name="d_id" value="@Model.d_id" />
    <label class="col-md-2">Name</label>
    <div class="col-md-4">
        <input type="text" name="d_name" class="form-control" value="@Model.d_name" />
    </div>
    <label class="col-md-2">Contact</label>
    <div class="col-md-4">
        <input type="text" name="contact" class="form-control" value="@Model.contact" />
    </div>
   

    <div class="col-md-2">
        <input type="submit" class="btn btn-success" value="Save" />
    </div>
}

